DEF_SND_STUFFA MACRO num
ACTION_SND_num EQU $
	ACTION_START	1,OPTION_STAND
	ACTION_PART  300+num, 1, 0, ACTBYTE_POSTURE_STAND
        ;; just special sound play motion
 ENDM

ADDR_ACTION_PT3: ; must be one after eachother
            ;; only accessed with YICT

 IFDEF LATER_SIT_TENDENCY
  ;; YICT203 maybe?

